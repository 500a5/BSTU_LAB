#include <msp430.h> 
#include "stdio.h"
#include "system_define.h"
#include "system_variable.h"
#include "function_prototype.h"
#include "main.h"

/*
 * main.c
 */
// speed = 38400 - скорость обмена
// databits = 8 - длинна символа
// stopbits = 1, 2 - кол-во передаваемых стоповых бит
// parity = 0 - без контроля четности
// iface = 0 - USB
void main(void) {
    WDTCTL = WDTPW + WDTHOLD;
    Init_System_Clock();
    Init_System();
    char s[50];
    char i = '20';
    word j;
    for(j = 0; j < 50; j++)
    {
        s[i] = i + j;
    }
    UART_init(38400, 8, 1, 0, 0);
    UART_message(s);

}
