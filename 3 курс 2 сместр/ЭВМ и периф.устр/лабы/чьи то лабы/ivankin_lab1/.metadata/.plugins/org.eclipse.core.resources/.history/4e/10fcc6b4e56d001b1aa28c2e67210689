#include <msp430.h> 
#include "stdio.h"
#include "system_define.h"
#include "system_variable.h"
#include "function_prototype.h"
#include "lcd.h"
#include "sysfunc.h"
#include "main.h"

/*
 * main.c
 */
void main(void)
{
    char name[15] = "Бобылев Максим\0";
    char gr[5] = "ВТ-32";
    WDTCTL = WDTPW + WDTHOLD;
    Init_System();
    Init_System_Clock();
    LCD_init();
    LCD_clear();
    LCD_set_cursor(3);
    LCD_message(name);
    LCD_set_pos(1, 0);
    LCD_message(gr);
}
