#include <msp430.h> 
#include "stdio.h"
#include "system_define.h"
#include "system_variable.h"
#include "function_prototype.h"
#include "main.h"

/*
 * main.c
 */
void main(void)
{
  WDTCTL = WDTPW + WDTHOLD;

  Init_System();

  Init_System_Clock();

  LCD_init();
  LCD_clear();

  char row1[] = "Ситников";
  char row2[] = "ВТ-32";

  LCD_message(row1);

  LCD_set_pos(1, 0);

  LCD_message(row2);
}
