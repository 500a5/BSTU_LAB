#include <msp430.h> 
#include "stdio.h"
#include "system_define.h"
#include "system_variable.h"
#include "function_prototype.h"
#include "main.h"
#include "lcd.h"
#include "sysfunc.h"
/*
 * main.c
 */
void main(void) {
    WDTCTL = WDTPW + WDTHOLD;
    Init_System();
    Init_System_Clock();
    LCD_init();
    LCD_clear();
    LCD_set_pos(0,0);
    LCD_message("Воскобойников Илья");
    LCD_set_pos(1,0);
    LCD_message("ВТ-32");
}
