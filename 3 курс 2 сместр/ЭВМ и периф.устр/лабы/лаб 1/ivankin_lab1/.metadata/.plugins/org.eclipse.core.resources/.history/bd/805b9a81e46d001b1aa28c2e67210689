#include <msp430.h> 
#include "stdio.h"
#include "system_define.h"
#include "system_variable.h"
#include "function_prototype.h"
#include "lcd.h"
#include "sysfunc.h"
#include "main.h"

/*
 * main.c
 */
void main(void)
{
    char name[20] = "Иванкин Кирилл ВТ-32";
    WDTCTL = WDTPW + WDTHOLDO;
    Init_System();
    Init_System_Clock();
    LCD_init();
    LCD_clear();
    LCD_message(name);
}
