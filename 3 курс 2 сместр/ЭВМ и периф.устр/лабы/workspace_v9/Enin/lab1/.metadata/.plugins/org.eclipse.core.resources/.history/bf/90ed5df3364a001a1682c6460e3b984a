#include <msp430.h> 
#include "stdio.h"
#include "system_define.h"
#include "system_variable.h"
#include "function_prototype.h"
#include "main.h"

/*
 * main.c
 */


void main(void)
{
    WDTCTL = WDTPW + WDTHOLD;
    Init_System_Clock();
    char* name = "Енин Георгий";
    char* groupe = "Группа ВТ-31";
    LCD_init();
    LCD_set_pos(0,0);
    LCD_message(groupe);
    LCD_set_pos(1,0);
    LCD_message(name);
//    char sym;
//    for (int i = 0; i < 8; i++)
//      for (int j = 0; j < 5; j++)
//          sym
//    LCD_WriteData(sym);
    while (1);
}
